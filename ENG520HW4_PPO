png":"\n"},"metadata":{}},{"output_type":"stream","name":"stdout","text":["\n","🎞️ Agent animation saved as 'frozenlake_agent.gif'\n","\n","Learned Policy with Lake Layout:\n","S → ↓ ←\n","↓ H ↓ H\n","→ → ↓ H\n","H → → G\n"]}]},{"cell_type":"code","source":["# ... (same imports and setup as before)\n","import gymnasium as gym\n","import torch\n","import torch.nn as nn\n","import torch.optim as optim\n","import numpy as np\n","import torch.nn.functional as F\n","import matplotlib.pyplot as plt\n","import imageio\n","\n","# === Hyperparameters ===\n","learning_rate = 2.5e-4\n","gamma = 0.99\n","clip_epsilon = 0.2\n","update_epochs = 4\n","batch_size = 32\n","timesteps_per_batch = 2048\n","max_timesteps = 100_000\n","\n","# === Environment setup ===\n","env = gym.make(\"FrozenLake-v1\", is_slippery=False, render_mode=\"rgb_array\")\n","state_size = env.observation_space.n\n","action_size = env.action_space.n\n","device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n","\n","def one_hot(state):\n","    return torch.eye(state_size)[state].to(device)\n","\n","# === Networks ===\n","class PolicyNetwork(nn.Module):\n","    def __init__(self):\n","        super().__init__()\n","        self.fc = nn.Linear(state_size, 64)\n","        self.out = nn.Linear(64, action_size)\n","\n","    def forward(self, x):\n","        x = F.relu(self.fc(x))\n","        return F.softmax(self.out(x), dim=-1)\n","\n","class ValueNetwork(nn.Module):\n","    def __init__(self):\n","        super().__init__()\n","        self.fc = nn.Linear(state_size, 64)\n","        self.out = nn.Linear(64, 1)\n","\n","    def forward(self, x):\n","        x = F.relu(self.fc(x))\n","        return self.out(x)\n","\n","policy = PolicyNetwork().to(device)\n","value_fn = ValueNetwork().to(device)\n","optimizer_policy = optim.Adam(policy.parameters(), lr=learning_rate)\n","optimizer_value = optim.Adam(value_fn.parameters(), lr=learning_rate)\n","\n","# === Data Collection ===\n","def collect_trajectories():\n","    obs_list, action_list, reward_list, done_list, logprob_list, value_list = [], [], [], [], [], []\n","    episode_lengths = []\n","    obs, _ = env.reset()\n","    steps = 0\n","\n","    for _ in range(timesteps_per_batch):\n","        state = one_hot(obs).unsqueeze(0)\n","        probs = policy(state)\n","        dist = torch.distributions.Categorical(probs)\n","        action = dist.sample()\n","        log_prob = dist.log_prob(action)\n","        value = value_fn(state)\n","\n","        next_obs, reward, done, truncated, _ = env.step(action.item())\n","\n","        obs_list.append(one_hot(obs))\n","        action_list.append(action)\n","        reward_list.append(reward)\n","        done_list.append(done or truncated)\n","        logprob_list.append(log_prob.detach())\n","        value_list.append(value.squeeze(0).detach())\n","\n","        obs = next_obs\n","        steps += 1\n","\n","        if done or truncated:\n","            episode_lengths.append(steps)\n","            steps = 0\n","            obs, _ = env.reset()\n","\n","    return (\n","        torch.stack(obs_list),\n","        torch.tensor([a.item() for a in action_list], dtype=torch.int64).to(device),\n","        reward_list,\n","        done_list,\n","        torch.stack(logprob_list),\n","        torch.stack(value_list),\n","        episode_lengths\n","    )\n","\n","def compute_gae(rewards, values, dones):\n","    returns = []\n","    gae = 0\n","    next_value = 0\n","    for i in reversed(range(len(rewards))):\n","        delta = rewards[i] + gamma * next_value * (1 - dones[i]) - values[i].item()\n","        gae = delta + gamma * 0.95 * (1 - dones[i]) * gae\n","        next_value = values[i].item()\n","        returns.insert(0, gae + values[i].item())\n","    return torch.tensor(returns, dtype=torch.float32).to(device)\n","\n","# === Training Loop ===\n","returns_history = []\n","length_history = []\n","timestep = 0\n","\n","while timestep < max_timesteps:\n","    states, actions, rewards, dones, log_probs_old, values, episode_lengths = collect_trajectories()\n","    timestep += len(rewards)\n","\n","    returns = compute_gae(rewards, values, dones).view(-1)\n","    values = values.view(-1)\n","    advantages = returns - values\n","    advantages = (advantages - advantages.mean()) / (advantages.std() + 1e-8)\n","\n","    for _ in range(update_epochs):\n","        for i in range(0, len(rewards), batch_size):\n","            batch_idx = slice(i, i + batch_size)\n","            batch_states = states[batch_idx].to(device)\n","            batch_actions = actions[batch_idx]\n","            batch_advantages = advantages[batch_idx]\n","            batch_returns = returns[batch_idx]\n","            batch_old_log_probs = log_probs_old[batch_idx]\n","\n","            probs = policy(batch_states)\n","            dist = torch.distributions.Categorical(probs)\n","            new_log_probs = dist.log_prob(batch_actions)\n","\n","            ratio = torch.exp(new_log_probs - batch_old_log_probs)\n","            surr1 = ratio * batch_advantages\n","            surr2 = torch.clamp(ratio, 1 - clip_epsilon, 1 + clip_epsilon) * batch_advantages\n","            loss_policy = -torch.min(surr1, surr2).mean()\n","\n","            loss_value = F.mse_loss(value_fn(batch_states).squeeze(), batch_returns)\n","\n","            optimizer_policy.zero_grad()\n","            loss_policy.backward()\n","            optimizer_policy.step()\n","\n","            optimizer_value.zero_grad()\n","            loss_value.backward()\n","            optimizer_value.step()\n","\n","    avg_return = np.mean(rewards)\n","    avg_length = np.mean(episode_lengths) if episode_lengths else 0\n","    returns_history.append(avg_return)\n","    length_history.append(avg_length)\n","    print(f\"Timesteps: {timestep}, Avg Return: {avg_return:.2f}, Avg Steps: {avg_length:.1f}\")\n","\n","env.close()\n","\n","# === Utility ===\n","def moving_average(data, window=10):\n","    return np.convolve(data, np.ones(window)/window, mode='valid')\n","\n","# === Reward Visualization ===\n","plt.figure(figsize=(12, 4))\n","plt.subplot(1, 2, 1)\n","plt.plot(returns_history, label=\"Avg Return\")\n","plt.title(\"Average Reward per Batch\")\n","plt.xlabel(\"Training Iteration\")\n","plt.ylabel(\"Reward\")\n","plt.grid(True)\n","\n","plt.subplot(1, 2, 2)\n","plt.plot(moving_average(returns_history), label=\"Smoothed\", color='orange')\n","plt.title(\"Smoothed Reward (Window=10)\")\n","plt.xlabel(\"Training Iteration\")\n","plt.ylabel(\"Reward\")\n","plt.grid(True)\n","plt.tight_layout()\n","plt.savefig(\"reward_curves.png\")\n","plt.show()\n","\n","# === Steps per Episode Plot (Raw + Smoothed) ===\n","plt.figure(figsize=(10, 5))\n","plt.plot(length_history, label=\"Avg Steps per Episode\", color=\"purple\", alpha=0.6)\n","plt.plot(moving_average(length_history), label=\"Smoothed\", color=\"darkred\", linewidth=2)\n","plt.title(\"Steps per Episode per Training Batch\")\n","plt.xlabel(\"Training Iteration\")\n","plt.ylabel(\"Steps\")\n","plt.grid(True)\n","plt.legend()\n","plt.tight_layout()\n","plt.savefig(\"steps_per_episode.png\")\n","plt.show()\n","\n","# === Policy Visualization ===\n","action_symbols = ['←', '↓', '→', '↑']\n","policy_actions = []\n","\n","for state in range(state_size):\n","    state_tensor = one_hot(state).unsqueeze(0)\n","    probs = policy(state_tensor)\n","    best_action = torch.argmax(probs).item()\n","    policy_actions.append(best_action)\n","\n","grid = np.array([action_symbols[a] for a in policy_actions]).reshape(4, 4)\n","print(\"\\nLearned Policy Grid (arrows only):\")\n","for row in grid:\n","    print(' '.join(row))\n","\n","desc = env.unwrapped.desc.astype(str)\n","enhanced_grid = np.copy(desc)\n","for i in range(4):\n","    for j in range(4):\n","        idx = i * 4 + j\n","        if desc[i, j] == 'F':\n","            enhanced_grid[i, j] = action_symbols[policy_actions[idx]]\n","\n","print(\"\\nLearned Policy with FrozenLake Layout:\")\n","for row in enhanced_grid:\n","    print(' '.join(row))\n","\n","plt.figure(figsize=(4, 4))\n","plt.title(\"Learned Policy\")\n","plt.imshow(np.zeros((4, 4)), cmap=\"Blues\", vmin=0, vmax=1)\n","for i in range(4):\n","    for j in range(4):\n","        plt.text(j, i, enhanced_grid[i, j], ha=\"center\", va=\"center\", fontsize=14)\n","plt.xticks([])\n","plt.yticks([])\n","plt.grid(False)\n","plt.savefig(\"policy_grid.png\")\n","plt.show()\n","\n","# === Agent Animation ===\n","frames = []\n","obs, _ = env.reset()\n","done = False\n","while not done:\n","    state = one_hot(obs).unsqueeze(0)\n","    with torch.no_grad():\n","        probs = policy(state)\n","        action = torch.argmax(probs).item()\n","    obs, _, done, truncated, _ = env.step(action)\n","    frame = env.render()\n","    frames.append(frame)\n","\n","imageio.mimsave(\"frozenlake_agent.gif\", frames, duration=0.5)\n","print(\"\\n🎞️ Agent animation saved as 'frozenlake_agent.gif'\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"BDJtw4rti35k","executionInfo":{"status":"ok","timestamp":1747859982446,"user_tz":420,"elapsed":119798,"user":{"displayName":"Matt J","userId":"11464525758576914107"}},"outputId":"38ffa0b9-c3e1-4b06-fa96-da4fee481ce9"},"execution_count":4,"outputs":[{"output_type":"stream","name":"stdout","text":["Timesteps: 2048, Avg Return: 0.00, Avg Steps: 8.2\n","Timesteps: 4096, Avg Return: 0.00, Avg Steps: 7.7\n","Timesteps: 6144, Avg Return: 0.00, Avg Steps: 7.8\n","Timesteps: 8192, Avg Return: 0.00, Avg Steps: 6.9\n","Timesteps: 10240, Avg Return: 0.00, Avg Steps: 7.5\n","Timesteps: 12288, Avg Return: 0.01, Avg Steps: 7.6\n","Timesteps: 14336, Avg Return: 0.01, Avg Steps: 8.2\n","Timesteps: 16384, Avg Return: 0.01, Avg Steps: 8.2\n","Timesteps: 18432, Avg Return: 0.01, Avg Steps: 8.1\n","Timesteps: 20480, Avg Return: 0.01, Avg Steps: 8.8\n","Timesteps: 22528, Avg Return: 0.01, Avg Steps: 8.7\n","Timesteps: 24576, Avg Return: 0.01, Avg Steps: 8.7\n","Timesteps: 26624, Avg Return: 0.01, Avg Steps: 9.3\n","Timesteps: 28672, Avg Return: 0.01, Avg Steps: 8.9\n","Timesteps: 30720, Avg Return: 0.01, Avg Steps: 8.7\n","Timesteps: 32768, Avg Return: 0.01, Avg Steps: 9.0\n","Timesteps: 34816, Avg Return: 0.01, Avg Steps: 8.6\n","Timesteps: 36864, Avg Return: 0.01, Avg Steps: 8.9\n","Timesteps: 38912, Avg Return: 0.01, Avg Steps: 8.3\n","Timesteps: 40960, Avg Return: 0.01, Avg Steps: 10.1\n","Timesteps: 43008, Avg Return: 0.01, Avg Steps: 9.6\n","Timesteps: 45056, Avg Return: 0.01, Avg Steps: 9.5\n","Timesteps: 47104, Avg Return: 0.01, Avg Steps: 9.2\n","Timesteps: 49152, Avg Return: 0.01, Avg Steps: 8.9\n","Timesteps: 51200, Avg Return: 0.01, Avg Steps: 9.3\n","Timesteps: 53248, Avg Return: 0.01, Avg Steps: 9.1\n","Timesteps: 55296, Avg Return: 0.02, Avg Steps: 9.0\n","Timesteps: 57344, Avg Return: 0.02, Avg Steps: 9.7\n","Timesteps: 59392, Avg Return: 0.02, Avg Steps: 9.2\n","Timesteps: 61440, Avg Return: 0.02, Avg Steps: 9.6\n","Timesteps: 63488, Avg Return: 0.01, Avg Steps: 8.9\n","Timesteps: 65536, Avg Return: 0.02, Avg Steps: 10.3\n","Timesteps: 67584, Avg Return: 0.02, Avg Steps: 9.2\n","Timesteps: 69632, Avg Return: 0.02, Avg Steps: 9.1\n","Timesteps: 71680, Avg Return: 0.02, Avg Steps: 9.3\n","Timesteps: 73728, Avg Return: 0.02, Avg Steps: 9.6\n","Timesteps: 75776, Avg Return: 0.02, Avg Steps: 8.9\n","Timesteps: 77824, Avg Return: 0.01, Avg Steps: 9.4\n","Timesteps: 79872, Avg Return: 0.01, Avg Steps: 9.3\n","Timesteps: 81920, Avg Return: 0.01, Avg Steps: 9.3\n","Timesteps: 83968, Avg Return: 0.01, Avg Steps: 9.2\n","Timesteps: 86016, Avg Return: 0.01, Avg Steps: 9.9\n","Timesteps: 88064, Avg Return: 0.01, Avg Steps: 9.4\n","Timesteps: 90112, Avg Return: 0.02, Avg Steps: 9.1\n","Timesteps: 92160, Avg Return: 0.02, Avg Steps: 9.7\n","Timesteps: 94208, Avg Return: 0.02, Avg Steps: 9.1\n","Timesteps: 96256, Avg Return: 0.02, Avg Steps: 9.1\n","Timesteps: 98304, Avg Return: 0.01, Avg Steps: 9.7\n","Timesteps: 100352, Avg Return: 0.01, Avg Steps: 10.3\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 1200x400 with 2 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"display_data","data":{"text/plain":["<Figure size 1000x500 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"stream","name":"stdout","text":["\n","Learned Policy Grid (arrows only):\n","↓ ↑ ↓ ←\n","↓ → ↓ →\n","→ ↓ ↓ →\n","→ → → →\n","\n","Learned Policy with FrozenLake Layout:\n","S ↑ ↓ ←\n","↓ H ↓ H\n","→ ↓ ↓ H\n","H → → G\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 400x400 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"stream","name":"stdout","text":["\n","🎞️ Agent animation saved as 'frozenlake_agent.gif'\n"]}]},{"cell_type":"code","source":["import gymnasium as gym\n","import torch\n","import torch.nn as nn\n","import torch.optim as optim\n","import numpy as np\n","import torch.nn.functional as F\n","import matplotlib.pyplot as plt\n","import imageio\n","from gymnasium.envs.toy_text.frozen_lake import generate_random_map\n","import os\n","\n","# === Create Random 7x7 FrozenLake Map ===\n","random_map = generate_random_map(size=7)\n","env = gym.make(\"FrozenLake-v1\", desc=random_map, is_slippery=False, render_mode=\"rgb_array\")\n","\n","# === Environment setup ===\n","state_size = env.observation_space.n\n","action_size = env.action_space.n\n","grid_size = int(np.sqrt(state_size))\n","device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n","\n","def one_hot(state):\n","    return torch.eye(state_size)[state].to(device)\n","\n","# === Networks ===\n","class PolicyNetwork(nn.Module):\n","    def __init__(self):\n","        super().__init__()\n","        self.fc = nn.Linear(state_size, 64)\n","        self.out = nn.Linear(64, action_size)\n","\n","    def forward(self, x):\n","        x = F.relu(self.fc(x))\n","        return F.softmax(self.out(x), dim=-1)\n","\n","class ValueNetwork(nn.Module):\n","    def __init__(self):\n","        super().__init__()\n","        self.fc = nn.Linear(state_size, 64)\n","        self.out = nn.Linear(64, 1)\n","\n","    def forward(self, x):\n","        x = F.relu(self.fc(x))\n","        return self.out(x)\n","\n","policy = PolicyNetwork().to(device)\n","value_fn = ValueNetwork().to(device)\n","optimizer_policy = optim.Adam(policy.parameters(), lr=2.5e-4)\n","optimizer_value = optim.Adam(value_fn.parameters(), lr=2.5e-4)\n","\n","# === Hyperparameters ===\n","gamma = 0.99\n","clip_epsilon = 0.2\n","update_epochs = 4\n","batch_size = 32\n","timesteps_per_batch = 2048\n","max_timesteps = 100_000\n","\n","# === Data Collection ===\n","def collect_trajectories():\n","    obs_list, action_list, reward_list, done_list, logprob_list, value_list = [], [], [], [], [], []\n","    obs, _ = env.reset()\n","    for _ in range(timesteps_per_batch):\n","        state = one_hot(obs).unsqueeze(0)\n","        probs = policy(state)\n","        dist = torch.distributions.Categorical(probs)\n","        action = dist.sample()\n","        log_prob = dist.log_prob(action)\n","        value = value_fn(state)\n","\n","        next_obs, reward, done, truncated, _ = env.step(action.item())\n","\n","        obs_list.append(one_hot(obs))\n","        action_list.append(action)\n","        reward_list.append(reward)\n","        done_list.append(done or truncated)\n","        logprob_list.append(log_prob.detach())\n","        value_list.append(value.squeeze(0).detach())\n","\n","        obs = next_obs if not done else env.reset()[0]\n","\n","    return (\n","        torch.stack(obs_list),\n","        torch.tensor([a.item() for a in action_list], dtype=torch.int64).to(device),\n","        reward_list,\n","        done_list,\n","        torch.stack(logprob_list),\n","        torch.stack(value_list),\n","    )\n","\n","def compute_gae(rewards, values, dones):\n","    returns = []\n","    gae = 0\n","    next_value = 0\n","    for i in reversed(range(len(rewards))):\n","        delta = rewards[i] + gamma * next_value * (1 - dones[i]) - values[i].item()\n","        gae = delta + gamma * 0.95 * (1 - dones[i]) * gae\n","        next_value = values[i].item()\n","        returns.insert(0, gae + values[i].item())\n","    return torch.tensor(returns, dtype=torch.float32).to(device)\n","\n","# === Training Loop ===\n","returns_history = []\n","timestep = 0\n","while timestep < max_timesteps:\n","    states, actions, rewards, dones, log_probs_old, values = collect_trajectories()\n","    timestep += len(rewards)\n","\n","    returns = compute_gae(rewards, values, dones).view(-1)\n","    values = values.view(-1)\n","    advantages = returns - values\n","    advantages = (advantages - advantages.mean()) / (advantages.std() + 1e-8)\n","\n","    for _ in range(update_epochs):\n","        for i in range(0, len(rewards), batch_size):\n","            batch_idx = slice(i, i + batch_size)\n","            batch_states = states[batch_idx].to(device)\n","            batch_actions = actions[batch_idx]\n","            batch_advantages = advantages[batch_idx]\n","            batch_returns = returns[batch_idx]\n","            batch_old_log_probs = log_probs_old[batch_idx]\n","\n","            probs = policy(batch_states)\n","            dist = torch.distributions.Categorical(probs)\n","            entropy = dist.entropy().mean()\n","            new_log_probs = dist.log_prob(batch_actions)\n","\n","            ratio = torch.exp(new_log_probs - batch_old_log_probs)\n","            surr1 = ratio * batch_advantages\n","            surr2 = torch.clamp(ratio, 1 - clip_epsilon, 1 + clip_epsilon) * batch_advantages\n","            loss_policy = -torch.min(surr1, surr2).mean()\n","\n","            loss_value = F.mse_loss(value_fn(batch_states).squeeze(), batch_returns)\n","\n","            optimizer_policy.zero_grad()\n","            loss_policy.backward()\n","            optimizer_policy.step()\n","\n","            optimizer_value.zero_grad()\n","            loss_value.backward()\n","            optimizer_value.step()\n","\n","    avg_return = np.mean(rewards)\n","    returns_history.append(avg_return)\n","    print(f\"Timesteps: {timestep}, Avg Return: {avg_return:.2f}\")\n","\n","env.close()\n","\n","# === Visualization: Reward Curve ===\n","plt.figure(figsize=(12, 4))\n","plt.subplot(1, 2, 1)\n","plt.plot(returns_history, label=\"Avg Return\")\n","plt.title(\"Average Reward per Batch\")\n","plt.xlabel(\"Training Iteration\")\n","plt.ylabel(\"Reward\")\n","plt.grid(True)\n","\n","def moving_average(data, window=10):\n","    return np.convolve(data, np.ones(window)/window, mode='valid')\n","\n","plt.subplot(1, 2, 2)\n","plt.plot(moving_average(returns_history), label=\"Smoothed\", color='orange')\n","plt.title(\"Smoothed Reward\")\n","plt.xlabel(\"Training Iteration\")\n","plt.ylabel(\"Reward\")\n","plt.grid(True)\n","plt.tight_layout()\n","plt.savefig(\"reward_curves.png\")\n","plt.show()\n","\n","# === Learned Policy Grid (arrows only) ===\n","action_symbols = ['←', '↓', '→', '↑']\n","policy_actions = []\n","\n","for state in range(state_size):\n","    state_tensor = one_hot(state).unsqueeze(0)\n","    probs = policy(state_tensor)\n","    best_action = torch.argmax(probs).item()\n","    policy_actions.append(best_action)\n","\n","grid = np.array([action_symbols[a] for a in policy_actions]).reshape(grid_size, grid_size)\n","\n","print(\"\\nLearned Policy Grid (arrows only):\")\n","for row in grid:\n","    print(' '.join(row))\n","\n","# === Overlay arrows on original FrozenLake map ===\n","desc = env.unwrapped.desc.astype(str)\n","enhanced_grid = np.copy(desc)\n","for i in range(grid_size):\n","    for j in range(grid_size):\n","        idx = i * grid_size + j\n","        if desc[i, j] == 'F':\n","            enhanced_grid[i, j] = action_symbols[policy_actions[idx]]\n","\n","print(\"\\nLearned Policy with FrozenLake Layout:\")\n","for row in enhanced_grid:\n","    print(' '.join(row))\n","\n","# === Save grid image ===\n","plt.figure(figsize=(6, 6))\n","plt.title(\"Learned Policy\")\n","plt.imshow(np.zeros((grid_size, grid_size)), cmap=\"Blues\")\n","for i in range(grid_size):\n","    for j in range(grid_size):\n","        plt.text(j, i, enhanced_grid[i, j], ha=\"center\", va=\"center\", fontsize=12)\n","plt.xticks([])\n","plt.yticks([])\n","plt.savefig(\"policy_grid.png\")\n","plt.show()\n","\n","# === Agent Animation ===\n","frames = []\n","obs, _ = env.reset()\n","done = False\n","while not done:\n","    state = one_hot(obs).unsqueeze(0)\n","    with torch.no_grad():\n","        probs = policy(state)\n","        action = torch.argmax(probs).item()\n","    obs, _, done, truncated, _ = env.step(action)\n","    frame = env.render()\n","    frames.append(frame)\n","\n","imageio.mimsave(\"frozenlake_agent.gif\", frames, duration=0.5)\n","print(\"\\n🎞️ Agent animation saved as 'frozenlake_agent.gif'\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"5hniAVcwXOC3","outputId":"c552d156-9c77-4314-fb6a-e85eb1c13e60"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Timesteps: 2048, Avg Return: 0.00\n","Timesteps: 4096, Avg Return: 0.00\n","Timesteps: 6144, Avg Return: 0.00\n","Timesteps: 8192, Avg Return: 0.00\n","Timesteps: 10240, Avg Return: 0.00\n","Timesteps: 12288, Avg Return: 0.00\n","Timesteps: 14336, Avg Return: 0.00\n","Timesteps: 16384, Avg Return: 0.00\n","Timesteps: 18432, Avg Return: 0.00\n","Timesteps: 20480, Avg Return: 0.00\n","Timesteps: 22528, Avg Return: 0.00\n","Timesteps: 24576, Avg Return: 0.00\n","Timesteps: 26624, Avg Return: 0.00\n","Timesteps: 28672, Avg Return: 0.00\n","Timesteps: 30720, Avg Return: 0.00\n","Timesteps: 32768, Avg Return: 0.00\n","Timesteps: 34816, Avg Return: 0.00\n","Timesteps: 36864, Avg Return: 0.00\n","Timesteps: 38912, Avg Return: 0.00\n","Timesteps: 40960, Avg Return: 0.00\n","Timesteps: 43008, Avg Return: 0.00\n","Timesteps: 45056, Avg Return: 0.00\n","Timesteps: 47104, Avg Return: 0.00\n","Timesteps: 49152, Avg Return: 0.00\n","Timesteps: 51200, Avg Return: 0.00\n","Timesteps: 53248, Avg Return: 0.00\n","Timesteps: 55296, Avg Return: 0.00\n","Timesteps: 57344, Avg Return: 0.00\n","Timesteps: 59392, Avg Return: 0.00\n","Timesteps: 61440, Avg Return: 0.00\n","Timesteps: 63488, Avg Return: 0.00\n","Timesteps: 65536, Avg Return: 0.00\n","Timesteps: 67584, Avg Return: 0.00\n","Timesteps: 69632, Avg Return: 0.00\n","Timesteps: 71680, Avg Return: 0.00\n","Timesteps: 73728, Avg Return: 0.00\n","Timesteps: 75776, Avg Return: 0.00\n","Timesteps: 77824, Avg Return: 0.00\n","Timesteps: 79872, Avg Return: 0.00\n","Timesteps: 81920, Avg Return: 0.00\n","Timesteps: 83968, Avg Return: 0.00\n","Timesteps: 86016, Avg Return: 0.00\n","Timesteps: 88064, Avg Return: 0.00\n","Timesteps: 90112, Avg Return: 0.00\n","Timesteps: 92160, Avg Return: 0.00\n","Timesteps: 94208, Avg Return: 0.00\n","Timesteps: 96256, Avg Return: 0.00\n","Timesteps: 98304, Avg Return: 0.00\n","Timesteps: 100352, Avg Return: 0.00\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 1200x400 with 2 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"stream","name":"stdout","text":["\n","Learned Policy Grid (arrows only):\n","← ← ← → → ← ↑\n","← ← ← ← → → ←\n","← ← ← → → ↑ →\n","→ ← ← → → ← ←\n","→ ← ← ← → → →\n","← → → ← → → ←\n","→ ← ← ← ← → ←\n","\n","Learned Policy with FrozenLake Layout:\n","S ← ← H H ← ↑\n","← ← ← ← H → ←\n","← ← ← → → ↑ →\n","→ ← ← H H ← ←\n","→ ← ← H → → H\n","H → H ← → → H\n","→ ← H ← ← → G\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 600x600 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["import gymnasium as gym\n","import torch\n","import torch.nn as nn\n","import torch.optim as optim\n","import numpy as np\n","import torch.nn.functional as F\n","import matplotlib.pyplot as plt\n","import matplotlib.animation as animation\n","from IPython.display import HTML\n","import imageio\n","import os\n","\n","# === Hyperparameters ===\n","learning_rate = 0.8\n","gamma = 0.95\n","clip_epsilon = 0.2\n","update_epochs = 4\n","batch_size = 32\n","timesteps_per_batch = 2048\n","max_timesteps = 100_000\n","\n","# === Environment setup ===\n","env = gym.make(\"FrozenLake-v1\", is_slippery=False, render_mode=\"rgb_array\")\n","state_size = env.observation_space.n\n","action_size = env.action_space.n\n","device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n","\n","def one_hot(state):\n","    return torch.eye(state_size)[state].to(device)\n","\n","# === Networks ===\n","class PolicyNetwork(nn.Module):\n","    def __init__(self):\n","        super().__init__()\n","        self.fc = nn.Linear(state_size, 64)\n","        self.out = nn.Linear(64, action_size)\n","\n","    def forward(self, x):\n","        x = F.relu(self.fc(x))\n","        return F.softmax(self.out(x), dim=-1)\n","\n","class ValueNetwork(nn.Module):\n","    def __init__(self):\n","        super().__init__()\n","        self.fc = nn.Linear(state_size, 64)\n","        self.out = nn.Linear(64, 1)\n","\n","    def forward(self, x):\n","        x = F.relu(self.fc(x))\n","        return self.out(x)\n","\n","policy = PolicyNetwork().to(device)\n","value_fn = ValueNetwork().to(device)\n","optimizer_policy = optim.Adam(policy.parameters(), lr=learning_rate)\n","optimizer_value = optim.Adam(value_fn.parameters(), lr=learning_rate)\n","\n","# === Data Collection ===\n","def collect_trajectories():\n","    obs_list, action_list, reward_list, done_list, logprob_list, value_list = [], [], [], [], [], []\n","    obs, _ = env.reset()\n","    for _ in range(timesteps_per_batch):\n","        state = one_hot(obs).unsqueeze(0)\n","        probs = policy(state)\n","        dist = torch.distributions.Categorical(probs)\n","        action = dist.sample()\n","        log_prob = dist.log_prob(action)\n","        value = value_fn(state)\n","\n","        next_obs, reward, done, truncated, _ = env.step(action.item())\n","\n","        obs_list.append(one_hot(obs))\n","        action_list.append(action)\n","        reward_list.append(reward)\n","        done_list.append(done or truncated)\n","        logprob_list.append(log_prob.detach())\n","        value_list.append(value.squeeze(0).detach())\n","\n","        obs = next_obs if not done else env.reset()[0]\n","\n","    return (\n","        torch.stack(obs_list),\n","        torch.tensor([a.item() for a in action_list], dtype=torch.int64).to(device),\n","        reward_list,\n","        done_list,\n","        torch.stack(logprob_list),\n","        torch.stack(value_list),\n","    )\n","\n","def compute_gae(rewards, values, dones):\n","    returns = []\n","    gae = 0\n","    next_value = 0\n","    for i in reversed(range(len(rewards))):\n","        delta = rewards[i] + gamma * next_value * (1 - dones[i]) - values[i].item()\n","        gae = delta + gamma * 0.95 * (1 - dones[i]) * gae\n","        next_value = values[i].item()\n","        returns.insert(0, gae + values[i].item())\n","    return torch.tensor(returns, dtype=torch.float32).to(device)\n","\n","# === Training Loop ===\n","returns_history = []\n","timestep = 0\n","while timestep < max_timesteps:\n","    states, actions, rewards, dones, log_probs_old, values = collect_trajectories()\n","    timestep += len(rewards)\n","\n","    returns = compute_gae(rewards, values, dones).view(-1)\n","    values = values.view(-1)\n","    advantages = returns - values\n","    advantages = (advantages - advantages.mean()) / (advantages.std() + 1e-8)\n","\n","    for _ in range(update_epochs):\n","        for i in range(0, len(rewards), batch_size):\n","            batch_idx = slice(i, i + batch_size)\n","            batch_states = states[batch_idx].to(device)\n","            batch_actions = actions[batch_idx]\n","            batch_advantages = advantages[batch_idx]\n","            batch_returns = returns[batch_idx]\n","            batch_old_log_probs = log_probs_old[batch_idx]\n","\n","            probs = policy(batch_states)\n","            dist = torch.distributions.Categorical(probs)\n","            entropy = dist.entropy().mean()\n","            new_log_probs = dist.log_prob(batch_actions)\n","\n","            ratio = torch.exp(new_log_probs - batch_old_log_probs)\n","            surr1 = ratio * batch_advantages\n","            surr2 = torch.clamp(ratio, 1 - clip_epsilon, 1 + clip_epsilon) * batch_advantages\n","            loss_policy = -torch.min(surr1, surr2).mean()\n","\n","            loss_value = F.mse_loss(value_fn(batch_states).squeeze(), batch_returns)\n","\n","            optimizer_policy.zero_grad()\n","            loss_policy.backward()\n","            optimizer_policy.step()\n","\n","            optimizer_value.zero_grad()\n","            loss_value.backward()\n","            optimizer_value.step()\n","\n","    avg_return = np.mean(rewards)\n","    returns_history.append(avg_return)\n","    print(f\"Timesteps: {timestep}, Avg Return: {avg_return:.2f}\")\n","\n","env.close()\n","\n","# === Visualization 1 & 2: Reward Curves ===\n","plt.figure(figsize=(12, 4))\n","plt.subplot(1, 2, 1)\n","plt.plot(returns_history, label=\"Avg Return\")\n","plt.title(\"Average Reward per Batch\")\n","plt.xlabel(\"Training Iteration\")\n","plt.ylabel(\"Reward\")\n","plt.grid(True)\n","\n","def moving_average(data, window=10):\n","    return np.convolve(data, np.ones(window)/window, mode='valid')\n","\n","plt.subplot(1, 2, 2)\n","plt.plot(moving_average(returns_history, 10), label=\"Smoothed\", color='orange')\n","plt.title(\"Smoothed Reward (Window=10)\")\n","plt.xlabel(\"Training Iteration\")\n","plt.ylabel(\"Reward\")\n","plt.grid(True)\n","plt.tight_layout()\n","plt.savefig(\"reward_curves.png\")\n","plt.show()\n","\n","# === Visualization 3: Policy Grid and Lake Map ===\n","action_symbols = ['←', '↓', '→', '↑']\n","policy_actions = []\n","\n","for state in range(state_size):\n","    state_tensor = one_hot(state).unsqueeze(0)\n","    probs = policy(state_tensor)\n","    best_action = torch.argmax(probs).item()\n","    policy_actions.append(best_action)\n","\n","grid = np.array([action_symbols[a] for a in policy_actions]).reshape(4, 4)\n","\n","print(\"\\nLearned Policy Grid (arrows only):\")\n","for row in grid:\n","    print(' '.join(row))\n","\n","# Overlay arrows on original FrozenLake map\n","desc = env.unwrapped.desc.astype(str)\n","enhanced_grid = np.copy(desc)\n","for i in range(4):\n","    for j in range(4):\n","        idx = i * 4 + j\n","        if desc[i, j] == 'F':\n","            enhanced_grid[i, j] = action_symbols[policy_actions[idx]]\n","\n","print(\"\\nLearned Policy with FrozenLake Layout:\")\n","for row in enhanced_grid:\n","    print(' '.join(row))\n","\n","# === Save grid image ===\n","plt.figure(figsize=(4, 4))\n","plt.title(\"Learned Policy\")\n","plt.imshow(np.zeros((4, 4)), cmap=\"Blues\", vmin=0, vmax=1)\n","for i in range(4):\n","    for j in range(4):\n","        plt.text(j, i, enhanced_grid[i, j], ha=\"center\", va=\"center\", fontsize=14)\n","plt.xticks([])\n","plt.yticks([])\n","plt.grid(False)\n","plt.savefig(\"policy_grid.png\")\n","plt.show()\n","\n","# === Animation ===\n","frames = []\n","obs, _ = env.reset()\n","done = False\n","while not done:\n","    state = one_hot(obs).unsqueeze(0)\n","    with torch.no_grad():\n","        probs = policy(state)\n","        action = torch.argmax(probs).item()\n","    obs, _, done, truncated, _ = env.step(action)\n","    frame = env.render()\n","    frames.append(frame)\n","\n","# Save as GIF\n","imageio.mimsave(\"frozenlake_agent.gif\", frames, duration=0.5)\n","print(\"\\n🎞️ Agent animation saved as 'frozenlake_agent.gif'\")\n","\n","\n","desc = env.unwrapped.desc.astype(str)\n","\n","enhanced_grid = np.copy(desc)\n","for i in range(4):\n","    for j in range(4):\n","        idx = i * 4 + j\n","        if desc[i, j] == 'F':\n","            enhanced_grid[i, j] = action_symbols[policy_actions[idx]]\n","\n","print(\"\\nLearned Policy with Lake Layout:\")\n","for row in enhanced_grid:\n","    print(' '.join(row))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"srve21nGcJ-F","outputId":"29cfa5ea-a35d-40ae-8794-0ffe80294259"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Timesteps: 2048, Avg Return: 0.00\n","Timesteps: 4096, Avg Return: 0.00\n","Timesteps: 6144, Avg Return: 0.00\n","Timesteps: 8192, Avg Return: 0.00\n","Timesteps: 10240, Avg Return: 0.00\n","Timesteps: 12288, Avg Return: 0.00\n","Timesteps: 14336, Avg Return: 0.00\n","Timesteps: 16384, Avg Return: 0.00\n","Timesteps: 18432, Avg Return: 0.00\n","Timesteps: 20480, Avg Return: 0.00\n","Timesteps: 22528, Avg Return: 0.00\n","Timesteps: 24576, Avg Return: 0.00\n","Timesteps: 26624, Avg Return: 0.00\n","Timesteps: 28672, Avg Return: 0.00\n","Timesteps: 30720, Avg Return: 0.00\n","Timesteps: 32768, Avg Return: 0.00\n","Timesteps: 34816, Avg Return: 0.00\n","Timesteps: 36864, Avg Return: 0.00\n","Timesteps: 38912, Avg Return: 0.00\n","Timesteps: 40960, Avg Return: 0.00\n","Timesteps: 43008, Avg Return: 0.00\n","Timesteps: 45056, Avg Return: 0.00\n","Timesteps: 47104, Avg Return: 0.00\n","Timesteps: 49152, Avg Return: 0.00\n","Timesteps: 51200, Avg Return: 0.00\n","Timesteps: 53248, Avg Return: 0.00\n","Timesteps: 55296, Avg Return: 0.00\n","Timesteps: 57344, Avg Return: 0.00\n","Timesteps: 59392, Avg Return: 0.00\n","Timesteps: 61440, Avg Return: 0.00\n","Timesteps: 63488, Avg Return: 0.00\n","Timesteps: 65536, Avg Return: 0.00\n","Timesteps: 67584, Avg Return: 0.00\n","Timesteps: 69632, Avg Return: 0.00\n","Timesteps: 71680, Avg Return: 0.00\n","Timesteps: 73728, Avg Return: 0.00\n","Timesteps: 75776, Avg Return: 0.00\n","Timesteps: 77824, Avg Return: 0.00\n","Timesteps: 79872, Avg Return: 0.00\n","Timesteps: 81920, Avg Return: 0.00\n","Timesteps: 83968, Avg Return: 0.00\n","Timesteps: 86016, Avg Return: 0.00\n","Timesteps: 88064, Avg Return: 0.00\n","Timest