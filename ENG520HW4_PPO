       x = F.relu(self.fc(x))\n","        return F.softmax(self.out(x), dim=-1)\n","\n","class ValueNetwork(nn.Module):\n","    def __init__(self):\n","        super().__init__()\n","        self.fc = nn.Linear(state_size, 64)\n","        self.out = nn.Linear(64, 1)\n","\n","    def forward(self, x):\n","        x = F.relu(self.fc(x))\n","        return self.out(x)\n","\n","policy = PolicyNetwork().to(device)\n","value_fn = ValueNetwork().to(device)\n","optimizer_policy = optim.Adam(policy.parameters(), lr=learning_rate)\n","optimizer_value = optim.Adam(value_fn.parameters(), lr=learning_rate)\n","\n","# === Data Collection ===\n","def collect_trajectories():\n","    obs_list, action_list, reward_list, done_list, logprob_list, value_list = [], [], [], [], [], []\n","    obs, _ = env.reset()\n","    for _ in range(timesteps_per_batch):\n","        state = one_hot(obs).unsqueeze(0)\n","        probs = policy(state)\n","        dist = torch.distributions.Categorical(probs)\n","        action = dist.sample()\n","        log_prob = dist.log_prob(action)\n","        value = value_fn(state)\n","\n","        next_obs, reward, done, truncated, _ = env.step(action.item())\n","\n","        obs_list.append(one_hot(obs))\n","        action_list.append(action)\n","        reward_list.append(reward)\n","        done_list.append(done or truncated)\n","        logprob_list.append(log_prob.detach())\n","        value_list.append(value.squeeze(0).detach())\n","\n","        obs = next_obs if not done else env.reset()[0]\n","\n","    return (\n","        torch.stack(obs_list),\n","        torch.tensor([a.item() for a in action_list], dtype=torch.int64).to(device),\n","        reward_list,\n","        done_list,\n","        torch.stack(logprob_list),\n","        torch.stack(value_list),\n","    )\n","\n","def compute_gae(rewards, values, dones):\n","    returns = []\n","    gae = 0\n","    next_value = 0\n","    for i in reversed(range(len(rewards))):\n","        delta = rewards[i] + gamma * next_value * (1 - dones[i]) - values[i].item()\n","        gae = delta + gamma * 0.95 * (1 - dones[i]) * gae\n","        next_value = values[i].item()\n","        returns.insert(0, gae + values[i].item())\n","    return torch.tensor(returns, dtype=torch.float32).to(device)\n","\n","# === Training Loop ===\n","returns_history = []\n","timestep = 0\n","while timestep < max_timesteps:\n","    states, actions, rewards, dones, log_probs_old, values = collect_trajectories()\n","    timestep += len(rewards)\n","\n","    returns = compute_gae(rewards, values, dones).view(-1)\n","    values = values.view(-1)\n","    advantages = returns - values\n","    advantages = (advantages - advantages.mean()) / (advantages.std() + 1e-8)\n","\n","    for _ in range(update_epochs):\n","        for i in range(0, len(rewards), batch_size):\n","            batch_idx = slice(i, i + batch_size)\n","            batch_states = states[batch_idx].to(device)\n","            batch_actions = actions[batch_idx]\n","            batch_advantages = advantages[batch_idx]\n","            batch_returns = returns[batch_idx]\n","            batch_old_log_probs = log_probs_old[batch_idx]\n","\n","            probs = policy(batch_states)\n","            dist = torch.distributions.Categorical(probs)\n","            entropy = dist.entropy().mean()\n","            new_log_probs = dist.log_prob(batch_actions)\n","\n","            ratio = torch.exp(new_log_probs - batch_old_log_probs)\n","            surr1 = ratio * batch_advantages\n","            surr2 = torch.clamp(ratio, 1 - clip_epsilon, 1 + clip_epsilon) * batch_advantages\n","            loss_policy = -torch.min(surr1, surr2).mean()\n","\n","            loss_value = F.mse_loss(value_fn(batch_states).squeeze(), batch_returns)\n","\n","            optimizer_policy.zero_grad()\n","            loss_policy.backward()\n","            optimizer_policy.step()\n","\n","            optimizer_value.zero_grad()\n","            loss_value.backward()\n","            optimizer_value.step()\n","\n","    avg_return = np.mean(rewards)\n","    returns_history.append(avg_return)\n","    print(f\"Timesteps: {timestep}, Avg Return: {avg_return:.2f}\")\n","\n","env.close()\n","\n","# === Visualization 1 & 2: Reward Curves ===\n","plt.figure(figsize=(12, 4))\n","plt.subplot(1, 2, 1)\n","plt.plot(returns_history, label=\"Avg Return\")\n","plt.title(\"Average Reward per Batch\")\n","plt.xlabel(\"Training Iteration\")\n","plt.ylabel(\"Reward\")\n","plt.grid(True)\n","\n","def moving_average(data, window=10):\n","    return np.convolve(data, np.ones(window)/window, mode='valid')\n","\n","plt.subplot(1, 2, 2)\n","plt.plot(moving_average(returns_history, 10), label=\"Smoothed\", color='orange')\n","plt.title(\"Smoothed Reward (Window=10)\")\n","plt.xlabel(\"Training Iteration\")\n","plt.ylabel(\"Reward\")\n","plt.grid(True)\n","plt.tight_layout()\n","plt.savefig(\"reward_curves.png\")\n","plt.show()\n","\n","# === Visualization 3: Policy Grid and Lake Map ===\n","action_symbols = ['←', '↓', '→', '↑']\n","policy_actions = []\n","\n","for state in range(state_size):\n","    state_tensor = one_hot(state).unsqueeze(0)\n","    probs = policy(state_tensor)\n","    best_action = torch.argmax(probs).item()\n","    policy_actions.append(best_action)\n","\n","grid = np.array([action_symbols[a] for a in policy_actions]).reshape(4, 4)\n","\n","print(\"\\nLearned Policy Grid (arrows only):\")\n","for row in grid:\n","    print(' '.join(row))\n","\n","# Overlay arrows on original FrozenLake map\n","desc = env.unwrapped.desc.astype(str)\n","enhanced_grid = np.copy(desc)\n","for i in range(4):\n","    for j in range(4):\n","        idx = i * 4 + j\n","        if desc[i, j] == 'F':\n","            enhanced_grid[i, j] = action_symbols[policy_actions[idx]]\n","\n","print(\"\\nLearned Policy with FrozenLake Layout:\")\n","for row in enhanced_grid:\n","    print(' '.join(row))\n","\n","# === Save grid image ===\n","plt.figure(figsize=(4, 4))\n","plt.title(\"Learned Policy\")\n","plt.imshow(np.zeros((4, 4)), cmap=\"Blues\", vmin=0, vmax=1)\n","for i in range(4):\n","    for j in range(4):\n","        plt.text(j, i, enhanced_grid[i, j], ha=\"center\", va=\"center\", fontsize=14)\n","plt.xticks([])\n","plt.yticks([])\n","plt.grid(False)\n","plt.savefig(\"policy_grid.png\")\n","plt.show()\n","\n","# === Animation ===\n","frames = []\n","obs, _ = env.reset()\n","done = False\n","while not done:\n","    state = one_hot(obs).unsqueeze(0)\n","    with torch.no_grad():\n","        probs = policy(state)\n","        action = torch.argmax(probs).item()\n","    obs, _, done, truncated, _ = env.step(action)\n","    frame = env.render()\n","    frames.append(frame)\n","\n","# Save as GIF\n","imageio.mimsave(\"frozenlake_agent.gif\", frames, duration=0.5)\n","print(\"\\n🎞️ Agent animation saved as 'frozenlake_agent.gif'\")\n","\n","\n","desc = env.unwrapped.desc.astype(str)\n","\n","enhanced_grid = np.copy(desc)\n","for i in range(4):\n","    for j in range(4):\n","        idx = i * 4 + j\n","        if desc[i, j] == 'F':\n","            enhanced_grid[i, j] = action_symbols[policy_actions[idx]]\n","\n","print(\"\\nLearned Policy with Lake Layout:\")\n","for row in enhanced_grid:\n","    print(' '.join(row))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"xjCq4tEzUngA","executionInfo":{"status":"ok","timestamp":1747856557031,"user_tz":420,"elapsed":114392,"user":{"displayName":"Matt J","userId":"11464525758576914107"}},"outputId":"9e401440-6acb-4cf7-8b07-f774abf4da88"},"execution_count":4,"outputs":[{"output_type":"stream","name":"stdout","text":["Timesteps: 2048, Avg Return: 0.00\n","Timesteps: 4096, Avg Return: 0.00\n","Timesteps: 6144, Avg Return: 0.00\n","Timesteps: 8192, Avg Return: 0.00\n","Timesteps: 10240, Avg Return: 0.00\n","Timesteps: 12288, Avg Return: 0.00\n","Timesteps: 14336, Avg Return: 0.01\n","Timesteps: 16384, Avg Return: 0.01\n","Timesteps: 18432, Avg Return: 0.01\n","Timesteps: 20480, A